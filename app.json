{
  "expo": {
    "name": "A hora do anjo",
    "slug": "ahda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/main/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/main/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CAEAF5"
    },
    "ios": {
      "bundleIdentifier": "com.hsvf.ahda",
      "supportsTablet": true,
      "requireFullScreen": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "package": "com.hsvf.ahda",
      "permissions": ["RECEIVE_BOOT_COMPLETED"]
    },
    "web": {
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/main/notification.png",
          "color": "#CAEAF5"
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "description": "Radio A hora do Anjo app, a Catholic radio station created in the city of Novo Repartimento.",
    "githubUrl": "https://github.com/HelvioFilho/ahda"
  }
}
